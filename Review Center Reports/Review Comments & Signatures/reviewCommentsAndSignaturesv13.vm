                #if( $reviewClosed && !$reviewAutoClose )
                  #compareEventDates( $revClosedEventDate )
                  #if( $printEvent )
                    #getDateTime($!revClosedEventDate, $timeZone) $!revClosedUser.fullName closed the review before deadline date and time.
                  #else 
                    No
                  #end 
                #else 
                  No
                #end 
                </p>
              </td>
            </tr>
